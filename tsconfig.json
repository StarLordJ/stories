{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "esnext",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
  },
  "baseUrl": "./src",
  "paths": {
    "*": [
      "*"
    ]
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}